# editorconfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
trim_trailing_whitespace = true

[*.md]
# double whitespace at end of line
# denotes a line break in Markdown
trim_trailing_whitespace = false
indent_size = 2

[*.tgn]
insert_final_newline = false

[README.md]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
